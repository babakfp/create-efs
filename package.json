{
    "name": "create-efs",
    "description": "A CLI tool to create SvelteKit apps.",
    "version": "1.3.1",
    "license": "MIT",
    "scripts": {
        "dev": "rimraf dist && tsc -w",
        "build": "rimraf dist && tsc",
        "prepublishOnly": "rimraf dist && tsc",
        "format": "prettier -w ."
    },
    "dependencies": {
        "@clack/prompts": "0.10.1",
        "node-stream-zip": "1.15.0",
        "nodejs-file-downloader": "4.13.0",
        "picocolors": "1.1.1"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.4.1",
        "@types/node": "22.15.21",
        "prettier": "3.5.3",
        "rimraf": "6.0.1",
        "typescript": "5.8.3"
    },
    "repository": "github:babakfp/create-efs",
    "main": "./dist/index.js",
    "bin": "./dist/index.js",
    "files": [
        "./dist/",
        "./templates/"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "keywords": [
        "cli",
        "svelte",
        "create",
        "template",
        "scaffold"
    ],
    "type": "module"
}
